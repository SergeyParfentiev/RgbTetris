<Application x:Class="LedMatrixDebugConsole.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="InformationIcon" UriSource="resources/information.png" />
        <BitmapImage x:Key="WarningIcon" UriSource="resources/warning.png" />
